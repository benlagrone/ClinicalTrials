@model IEnumerable<ClinicalTrials.Data.Protocol>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Protocols</title>
</head>
<body>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProtocolId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NCTNum)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NCTNum_1)
            </th>
            <th>
            </th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ProtocolId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NCTNum)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NCTNum_1)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id=item.Id })
            </td>
        </tr>
    }
    </table>
    @if (ViewBag.next > 1) { @Html.ActionLink("Previous", null, new { start = ViewBag.next-2, count = ViewBag.count})}
    @if (ViewBag.more) { @Html.ActionLink("More", null, new { start = ViewBag.next, count = ViewBag.count })}


</body>
</html>
